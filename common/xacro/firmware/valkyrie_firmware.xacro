    <xacro:property name="HipPitchProcFirmware" value="${TurboProcRotaryFirmwareExtension}" />
    <xacro:property name="HipPitchFPGAFirmware" value="${TurboFPGAFirmwareNode3Extension}" />
    <xacro:property name="KneePitchProcFirmware" value="${TurboProcRotaryFirmwareExtension}" />
    <xacro:property name="KneePitchFPGAFirmware" value="${TurboFPGAFirmwareNode4Extension}" />
    <xacro:property name="AnkleRightActuatorProcFirmware" value="${TurboProcLinearFirmwareExtension}" />
