    <xacro:property name="AnkleLeftActuatorProcFirmware" value="${TurboProcLinearFirmwareExtension}" />
    <xacro:property name="AnkleLeftActuatorFPGAFirmware" value="${TurboFPGAFirmwareNode6Extension}" />

    <xacro:property name="LeftShoulderPitchProcFirmware" value="${TurboProcRotaryFirmwareExtension}" />
