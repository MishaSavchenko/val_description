<?xml version="1.0" ?>
<robot name="valkyrie_firmware_locations" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:property name="PublicProcFirmwareBaseURL" value="http://linkthatdoesnotresolve.com"/>
    <xacro:property name="PrivateProcFirmwareBaseURL" value="http://flexo.jsc.nasa.gov/bamboo/browse/EMB-SEAP/latest/artifact/shared/"/>

    <xacro:property name="PublicFPGAFirmwareBaseURL" value="http://linkthatdoesnotresolve.com"/>
    <xacro:property name="PrivateFPGAFirmwareBaseURL" value="http://flexo.jsc.nasa.gov/bamboo/browse/EMB-TURD/latest/artifact/BS/SBF-File/"/>

    <xacro:property name="TurboProcRotaryFirmwareExtension" value="Rotary-Binary-for-Bootloader/turbo_bootloader.bin"/>
    <xacro:property name="TurboProcLinearFirmwareExtension" value="Linear-Binary-for-Bootloader/turbo_bootloader.bin"/>

    <xacro:property name="TurboFPGAFirmwareNode1Extension" value="V1_turbodriver_daria1_c0n1.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode2Extension" value="V1_turbodriver_daria1_c0n2.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode3Extension" value="V1_turbodriver_daria1_c0n3.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode4Extension" value="V1_turbodriver_daria1_c0n4.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode5Extension" value="V1_turbodriver_daria1_c0n5.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode6Extension" value="V1_turbodriver_daria1_c0n6.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode7Extension" value="V1_turbodriver_daria1_c0n7.sbf.gz"/>
    <xacro:property name="TurboFPGAFirmwareNode8Extension" value="V1_turbodriver_daria1_c0n8.sbf.gz"/>

    <xacro:macro name="firmware_base_url" >
        <TurbodriverProcessorFirmware>
            <PublicProcFirmwareBaseURL id="${PublicProcFirmwareBaseURL}"/>
            <PrivateProcFirmwareBaseURL id="${PrivateProcFirmwareBaseURL}"/>
        </TurbodriverProcessorFirmware>
        <TurbodriverFPGAFirmware>
            <PublicFPGAFirmwareBaseURL id="${PublicFPGAFirmwareBaseURL}"/>
            <PrivateFPGAFirmwareBaseURL id="${PrivateFPGAFirmwareBaseURL}"/>
        </TurbodriverFPGAFirmware>
    </xacro:macro>

    <xacro:property name="LeftHipYawProcFirmware" value="${TurboProcRotaryFirmwareExtension}" />
    <xacro:property name="LeftHipYawFPGAFirmware" value="${TurboFPGAFirmwareNode3Extension}" />
    <xacro:property name="RightHipYawProcFirmware" value="${TurboProcRotaryFirmwareExtension}" />
    <xacro:property name="RightHipYawFPGAFirmware" value="${TurboFPGAFirmwareNode5Extension}" />
